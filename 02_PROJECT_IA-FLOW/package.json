{
    "name": "ia-flow",
    "version": "1.0.0",
    "description": "AI-powered software development flow based on ISO 9001",
    "main": "backend/server.js",
    "type": "module",
    "scripts": {
        "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
        "dev:backend": "nodemon backend/server.js",
        "dev:frontend": "vite frontend --port 5173",
        "start": "node backend/server.js",
        "build": "vite build frontend"
    },
    "dependencies": {
        "@google/generative-ai": "^0.1.3",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-rate-limit": "^7.1.5",
        "groq-sdk": "^0.3.3",
        "helmet": "^8.1.0",
        "stripe": "^14.10.0",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "concurrently": "^8.2.2",
        "nodemon": "^3.0.2",
        "vite": "^5.0.10"
    },
    "keywords": [
        "ai",
        "flow",
        "iso9001",
        "software-development"
    ],
    "author": "",
    "license": "MIT"
}
